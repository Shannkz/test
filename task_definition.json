{
  "family": "worker",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ],
  "containerDefinitions": [
    {
      "name": "worker",
      "image": "PLACEHOLDER",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8082,
          "hostPort": 8082,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "awslogs-fe-events",
          "awslogs-region": "us-central-1",
          "awslogs-stream-prefix": "awslogs-events"
        }
      }
    }
  ]
}